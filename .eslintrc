{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "env": {
    "jest": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "strict": 0,
    'react/jsx-filename-extension': [1, {extensions: ['.js']}],
    "prettier/prettier": [
      "error",
      {
        singleQuote: true,
        trailingComma: "all",
        bracketSpacing: false,
        jsxBracketSameLine: true,
      }
    ],
  }
}
